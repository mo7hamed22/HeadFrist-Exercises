package teat;

/**
 * @author mohamed
 *
 */
import java.util.*;
public class Just_test {
	/**
	 * @param args
	 */
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner in = new Scanner(System.in);
		int n ,c=0  ;
		n =in.nextInt();
		int arr []=new int[n];	   
		for(int i=1 ; i<n ; i++)
		{
			arr[i] =in.nextInt();
		}
		for(int i=1 ; i<n ; i++)
		{
			
			if(arr[i]<10&&arr[i+1]<10)
			{
				
				continue;
				
			}
			else c++;
			if(arr[i]==10&&arr[i+1]==10)
			{
			 continue;	
			}
			else c++;
			System.out.print(c);
			
			
		}
	    
		}
	}
	
